{
    "name": "vanner",
    "version": "2.2.0",
    "description": "command cli",
    "module": "bundle/main.js",
    "type": "module",
    "license": "MIT",
    "repository": "https://github.com/vanner-young/vanner.git",
    "author": "vanneryoung@gmail.com",
    "bin": {
        "vanner": "bundle/main.js"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "files": [
        "bundle/**/*",
        "package.json",
        "README.md"
    ],
    "keywords": [
        "cli",
        "commander",
        "template",
        "git",
        "manager",
        "cmd",
        "dos"
    ],
    "scripts": {
        "build": "bun exec 'NODE_ENV=production bun run bun.build.ts'",
        "prepublishOnly": "bun run build"
    },
    "workspaces": [
        "packages/*"
    ],
    "devDependencies": {
        "@types/bun": "latest",
        "@vanner/core": "workspace:*"
    },
    "peerDependencies": {
        "typescript": "^5"
    },
    "dependencies": {
        "@inquirer/prompts": "^8.1.0",
        "commander": "^14.0.2",
        "dotenv": "^17.2.3",
        "ini": "^6.0.0",
        "mv-common": "^1.2.4"
    }
}
